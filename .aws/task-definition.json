{
    "family": "Docker-Image-Configuration-Task-1",
    "containerDefinitions": [
        {
            "name": "python-app-container",
            "image": "339712892155.dkr.ecr.us-east-1.amazonaws.com/project-1:82",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "container-port",
